import arcade

arcade.open_window(600, 600, "My picture")
arcade.set_background_color((255, 188, 217))
arcade.start_render()

arcade.draw_lrtb_rectangle_filled(5, 35, 590, 570, arcade.color.BITTER_LIME)
arcade.draw_rectangle_filled(35, 570, 30, 20, (0, 255, 255, 100))
arcade.draw_rectangle_outline(100, 320, 45, 25, arcade.color.BLUSH,3)
arcade.draw_rectangle_filled(300, 300, 200, 50, (255, 0, 255), 90)
arcade.draw_point(50, 580, arcade.color.RED, 5)
arcade.draw_line(0, 600, 600, 0, arcade.color.BLACK, 2)
arcade.draw_line(600, 600, 0, 0, arcade.color.BLACK, 2)

arcade.finish_render()
arcade.run()
